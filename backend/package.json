{
  "name": "open-BF720",
  "version": "1.0.0",
  "description": "Client for retrieving measurements from BF720 scale",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "heroku-postbuild": "npm run build",
    "start": "nodemon",
    "inspect": "nodemon --inspect src/app.ts",
    "test": "jest",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix",
    "update": "git fetch && git rebase",
    "pm2-deploy": "sudo pm2 start --name $npm_package_name ./build/app.js",
    "pm2-remove": "sudo pm2 stop $npm_package_name && sudo pm2 delete $npm_package_name",
    "pm2-save": "sudo pm2 save",
    "pm2-restart": "sudo pm2 restart $npm_package_name"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "serus",
  "license": "MIT",
  "dependencies": {
    "@abandonware/bluetooth-hci-socket": "^0.5.3-7",
    "@abandonware/noble": "^1.9.2-10",
    "@types/noble": "0.0.40",
    "body-parser": "^1.19.0",
    "celebrate": "^13.0.3",
    "console-stamp": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-pino-logger": "^5.0.0",
    "node-cron": "^2.0.3",
    "pino": "^6.8.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/node": "14.0.13",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "tslint": "^5.20.1",
    "typescript": "^4.0.3"
  },
  "bugs": {
    "url": ""
  },
